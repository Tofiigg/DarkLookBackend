@model Contact
@{
    ViewData["Title"] = "Update";
    Layout = "~/Areas/Manage/Views/Shared/_Layout.cshtml";
}

<form method="post" enctype="multipart/form-data">
    
    <div class="mb-3">
        <label asp-for="@Model.Location"></label>
        <input class="form-control" asp-for="@Model.Location">
        <span class="alert-danger" asp-validation-for="@Model.Location"></span>
    </div>
    <div class="mb-3">
        <label asp-for="@Model.Number"></label>
        <input class="form-control" asp-for="@Model.Number">
        <span class="alert-danger" asp-validation-for="@Model.Number"></span>
    </div>
    <div class="mb-3">
        <label asp-for="@Model.Email"></label>
        <input class="form-control" asp-for="@Model.Email">
        <span class="alert-danger" asp-validation-for="@Model.Email"></span>
    </div>
    <div class="mb-3">
        <label asp-for="@Model.Careers"></label>
        <input class="form-control" asp-for="@Model.Careers">
        <span class="alert-danger" asp-validation-for="@Model.Careers"></span>
    </div>
    <div class="mb-3">
        <label asp-for="@Model.SayHello"></label>
        <input class="form-control" asp-for="@Model.SayHello">
        <span class="alert-danger" asp-validation-for="@Model.SayHello"></span>
    </div>
    <div class="mb-3">
        <label asp-for="@Model.InfoEmail"></label>
        <input class="form-control" asp-for="@Model.InfoEmail">
        <span class="alert-danger" asp-validation-for="@Model.InfoEmail"></span>
    </div>


    <button type="submit" class="btn btn-primary">Submit</button>
</form>

